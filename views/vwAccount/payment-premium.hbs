{{#section 'js'}}
<script src="https://cdn.jsdelivr.net/npm/cleave.js@1.6.0/dist/cleave.min.js"></script>
<script>
    new Cleave('#txtCardNumber', {
        blocks: [4, 4, 4],
        delimiter: ' ',
        numericOnly: true
    });

    new Cleave('#txtExpiration', {
        date: true,
        datePattern: ['m', 'y']
    });

    new Cleave('#txtCVV', {
        blocks: [3],
        numericOnly: true
    });
</script>
{{/section}}

