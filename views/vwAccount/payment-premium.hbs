                            <!-- 2 column grid layout with text inputs for the Expiration and Security code -->
                            <div class="row">
                                <div class="col-md-6 mb-4">
                                    <div data-mdb-input-init class="form-outline text-muted" style="text-align: left;">
                                        <label class="form-label" for="txtExpiration">Expiration</label>
                                        <input type="text" id="txtExpiration" class="form-control" placeholder="MM/yy"
                                            required>
                                    </div>
                                </div>
                                <div class="col-md-6 mb-4">
                                    <div data-mdb-input-init class="form-outline text-muted" style="text-align: left;">
                                        <label class="form-label" for="txtCVV">Security code</label>
                                        <input type="text" id="txtCVV" class="form-control" placeholder="123" required>
                                    </div>
                                </div>
                            </div>
